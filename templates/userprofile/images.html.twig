     <!-- ======= Portfolio Section ======= --> 
     <section id="portfolio" class="portfolio">
      <div class="container">

        <div class="section-title">
          <h2>Votre Gallerie</h2>
          
        </div>

        <div class="row">
          <div class="col-lg-6 d-flex justify-content-center">
            <ul id="portfolio-flters">
              <li data-filter="*" class="filter-active">Tous</li>
              <li data-filter=".filter-app">Camping</li>
              <li data-filter=".filter-card">Randonnées</li>
              {# <li data-filter=".filter-web">Web</li> #}
            </ul>
          </div>
          <div class="col-lg-6 d-flex justify-content-right">
            <button type="button" class="mod1" data-toggle="modal" data-target="#myModal" style="margin-left:300px;">Ajouter une photo</button>
          </div>
        </div>

        <div class="row portfolio-container">
          {% for image in photo %}
            {% if image.type == "Camping" %}
          <div class="col-lg-4 col-md-6 portfolio-item filter-app">
            <div class="portfolio-wrap">
              <img src="/uploads/personne/{{image.photo}}" class="img-fluid" alt="">
              <div class="portfolio-info">
                <h4>{{image.titre}}</h4>
                <p> {{image.description}}.</p>
                <div class="portfolio-links">
                  <a href="/uploads/personne/{{image.photo}}" data-gallery="portfolioGallery" class="portfolio-lightbox" title={{image.titre}}><i class="bx bx-plus"></i></a>
                  <a href="#" title="More Details"><i class="bx bx-link"></i></a>
                </div>
              </div>
            </div>
          </div>
          {% endif %}

          
           {% if image.type == "Randonnée" %}
          <div class="col-lg-4 col-md-6 portfolio-item filter-card">
            <div class="portfolio-wrap">
              <img src="/uploads/personne/{{image.photo}}" class="img-fluid" alt="">
              <div class="portfolio-info">
                <h4>{{image.titre}} </h4>
                <p>{{image.description}}.</p>
                <div class="portfolio-links">
                  <a href="/uploads/personne/{{image.photo}}" data-gallery="portfolioGallery" class="portfolio-lightbox" title="{{image.titre}}"><i class="bx bx-plus"></i></a>
                  <a href="#" title="More Details"><i class="bx bx-link"></i></a>
                </div>
              </div>
            </div>
          </div>
          {% endif %}

          

          

         

         

          

          
          {% endfor %}
        </div>

      </div>
    </section><!-- End Portfolio Section -->

     
  
          
        
      

    
 
 

