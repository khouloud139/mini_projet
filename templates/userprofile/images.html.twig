        </div>
        <div class="modal-body">
          {{ form_start(PhotoForm) }}
          {{ form_widget(PhotoForm.photofile) }}
          {{ form_row(PhotoForm.titre) }}
         {{ form_row(PhotoForm.type) }}
        {{ form_row(PhotoForm.description) }}
          <button type="submit" class="mod1" style="margin-left:385px; margin-top:20px;" >Ajouter</button>
          {{ form_end(PhotoForm) }}
          
        </div>
        
      </div>
      
    </div>
