<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
		<title>Campa Holic</title>
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		 <link href="/assets/img/tent.jpg" rel="icon">
		
		<!-- MATERIAL DESIGN ICONIC FONT -->
		<link rel="stylesheet" href="/assets/fonts/material-design-iconic-font/css/material-design-iconic-font.min.css">

		<!-- STYLE CSS -->
		<link rel="stylesheet" href="/assets/css/login.css">
	</head>

	<body>

		<div class="wrapper" style="background:rgba(0,0,0,0.20)url('/assets/img/plan.jpg'); background-repeat: no-repeat;
		overflow: hidden;
		background-size: cover;
		background-blend-mode: darken;">
			<div class="inner">
				<div class="image-holder">
					<img src="/assets/img/lo.jpg" alt="">
				</div>
				{{ form_start(registrationForm) }}
					<h3 style="color:#026b04;">S'inscrire</h3>
					<div class="form-group">
						{{ form_widget(registrationForm.username) }}
						
						{{ form_widget(registrationForm.userlastname) }}
						
					</div>
					
					<div class="form-wrapper">
						{{ form_widget(registrationForm.email) }}
						<i class="zmdi zmdi-email"></i>
					</div>
					<div class="form-wrapper">
						{{ form_widget(registrationForm.adresse) }}
						<i class="zmdi zmdi-pin"></i>
					</div>
					<div class="form-wrapper">
						{{ form_widget(registrationForm.numtel) }}
						<i class="zmdi zmdi-phone"></i>
					</div>
					
					
					<div class="form-wrapper">
						{{ form_widget(registrationForm.plainPassword) }}
						<i class="zmdi zmdi-lock"></i>
					</div>
					
					<div class="form-wrapper">
						{{ form_row(registrationForm.photo) }}
						
					</div>
					<div class="form-wrapper">
						{{ form_row(registrationForm.agreeTerms) }}
						
					</div>
					<button type="submit" style="background-color:#026b04;">S'inscrire
						<i class="zmdi zmdi-arrow-right"></i>
					</button>
					{{ form_end(registrationForm) }}
			</div>
		</div>
		<script src="/aasets/js/jquery.min.js"></script>
  <script src="/aasets/js/popper.js"></script>
  <script src="/aasets/js/bootstrap.min.js"></script>
  <script src="/aasets/js/main2.js"></script>

		
	</body><!-- This templates was made by Colorlib (https://colorlib.com) -->
</html>