<!doctype html>
<html lang="en">
  <head>
  	<title>Login </title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

	<link href="https://fonts.googleapis.com/css?family=Lato:300,400,700&display=swap" rel="stylesheet">

	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
	
	<link rel="stylesheet" href="/assets/css/stylelog.css">

	</head>
	<header>
	
	<h1 class="logo">Camping</h1>
    
	</header>
	<body class="img js-fullheight" >
	<section class="ftco-section1">
		<div class="container1">
			
			<div class="row justify-content-center">
				<div class="col-md-6 col-lg-4">
					<div class="login-wrap p-0">
		      	<h3 class="mb-4 text-center">S'inscrire</h3>
		      	
    {{ form_start(registrationForm) }}
				
		      		<div class="form-group">
		      			<!--<input type="text" class="form-control" placeholder="Username" required>!-->
                        
						{{ form_widget(registrationForm.username) }}
		      		</div>
	            <div class="form-group">
	              <!--<input id="password-field" type="password" class="form-control" placeholder="Password" required>!-->
				  {{ form_widget(registrationForm.userlastname) }}
                 
	              
	            </div>
                <div class="form-group">
	              {{ form_widget(registrationForm.email) }}
                 
	              
	            </div>
                <div class="form-group">
	              <!--<input id="password-field" type="password" class="form-control" placeholder="Password" required>!-->
				{{ form_widget(registrationForm.adresse) }}
                 
	              
	            </div>
                <div class="form-group">
	              <!--<input id="password-field" type="password" class="form-control" placeholder="Password" required>!-->
				  {{ form_widget(registrationForm.numtel) }}
                 
	              
	            </div>
                <div class="form-group">
	              <!--<input id="password-field" type="password" class="form-control" placeholder="Password" required>!-->
				 {{ form_widget(registrationForm.plainPassword) }}

                 
	              
	            </div>
				<div class="form-group">
	              <!--<input id="password-field" type="password" class="form-control" placeholder="Password" required>!-->
				 {{ form_row(registrationForm.photo) }}
                <div class="form-group">
	              <!--<input id="password-field" type="password" class="form-control" placeholder="Password" required>!-->
				  {{ form_row(registrationForm.agreeTerms) }}
                 
	              
	            </div>
	            <div class="form-group">
	            	<button type="submit" class="form-control btn btn-primary submit px-3">S'inscrire</button>
	            </div>
	            <div class="form-group d-md-flex">
	            	
	           {{ form_end(registrationForm) }}
	          
		      </div>
				</div>
			</div>
		</div>
	</section>

	<script src="/aasets/js/jquery.min.js"></script>
  <script src="/aasets/js/popper.js"></script>
  <script src="/aasets/js/bootstrap.min.js"></script>
  <script src="/aasets/js/main2.js"></script>

	</body>
</html>


